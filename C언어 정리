//기본 기초 세팅
#include <stdio.h> //include:파일 '라이브러리'를 프로그램에 포함, stdio.h:파일 이름, .h:파일 확장자
int main(){ //int:정수(integer)=저장 변수-변수 '선언'
			//main이라는 이름 함수 정의-시작 시 함수 실행, 종료 시 함수 종료
			//() = 함수 호출
			//; = 문장 끝 마침표
	return 0;
	}
	
	
	// 정수형 자료형(변수 선언)
	char  //byte:1 bit:8 범위:-128~127
	short //byte:2 bit:16 범위:-32,768~32,767
	long //byte:4 bit:32 범위:-2,147,438,648~2,147,438,647
	int //byte:4 bit:32 범위:-2,147,438,648~2,147,438,647
	long long //byte:8 bit:64 범위:-9.22*1000000000000000000~-9.22*1000000000000000000
	
	//부호 없는 정수형 자료형(변수 선언)
	unsigned 정수형 자료형
	
	//실수형 자료형(변수 선언)
	float //byte:4 bit:32 범위:7~8
	double //byte:8 bit:64 범위:15~16
	
	
	//출력
	printf("출력 결과");
	//ex
	printf("hello");
	
	//서식 지정자 - 변수의 값 출력
	printf("서식 지정자", 변수);
	//정수 출력
	printf("%d", n);
	printf("%lld", n);
	//실수 출력
	printf("%f", n);
	printf("%lf", n);
	//문자 출력
	printf("%c", n);
	//문자열 출력
	printf("%s", n);
	
	
	//sizeof 연산자-자료형, 변수 크기 확인 가능
	printf("%d", sizeof(a));


	//형변환-서로 다른 자료형 사이 변환
	int a = 3.14;
	float b = 10;
	printf("%d %f", a, b);
	//결과값: a=3, b=10.000000	
	
	
	//문자 대응 정숫값
	%c = '2'
	%d = 50
	
	%c = a
	%d = 65
	
	
	//연산자
	//add
	z = x + y;
	//sub
	z = x - y;
	//mul
	z = x * y;
	//div
	z = x / y;
	//mod
	z = x % y;
	
	//복합 대입 연산자
	a += 1;
	a -= 1;
	a *= 1;
	a /= 1;
	a %= 1;
	
	//전치 증감 연산자(변수, 수)
	++a; //a만큼 더하기
	--a; //a만큼 빼기
	//후치 증감 연산자(대입 또는 윗줄 코드 실행 후)
	a++;
	a--;
	
	
	//비교 연산자
	== //같다
	!= //다르다
	> //크다
	< //작다
	>= //크거나 같다
	<= //작거나 같다
	
	//bool 자료형-진리 갑 저장용 자료형-진리
	bool z = x 비교 연산자 y;
	
	
	